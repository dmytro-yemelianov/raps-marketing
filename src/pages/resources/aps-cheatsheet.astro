---
import BaseLayout from '../../layouts/BaseLayout.astro';
import { getEntry } from 'astro:content';

const cheatsheet = await getEntry('cheatsheets', 'aps-cheatsheet');
const { Content } = await cheatsheet.render();
---

<BaseLayout 
  title="APS Developer Cheat Sheet" 
  description="Complete single-page reference for Autodesk Platform Services APIs with RAPS CLI shortcuts. OAuth flows, endpoints, error codes, and best practices."
>
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
    <div class="prose prose-lg max-w-none">
      <Content />
    </div>
  </div>
</BaseLayout>