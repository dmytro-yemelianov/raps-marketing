{
  "$ref": "#/definitions/videos",
  "definitions": {
    "videos": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "demo",
            "tutorial",
            "marketing",
            "comparison"
          ]
        },
        "audience": {
          "type": "string",
          "enum": [
            "all",
            "developer",
            "enterprise",
            "decision-maker"
          ]
        },
        "priority": {
          "type": "string",
          "enum": [
            "low",
            "medium",
            "high"
          ],
          "default": "medium"
        },
        "lastUpdated": {
          "anyOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "type": "string",
              "format": "date"
            },
            {
              "type": "integer",
              "format": "unix-time"
            }
          ]
        },
        "$schema": {
          "type": "string"
        }
      },
      "required": [
        "title",
        "description",
        "type",
        "audience",
        "lastUpdated"
      ],
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}